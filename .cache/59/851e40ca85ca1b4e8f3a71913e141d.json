0px;\n\t\t\tfont-family: var(--headline);\n\t\t\tpadding: 10px 40px;\n\t\t\tmargin-top: 20px;\n\t\t\tvertical-align: middle;\n\t\t\tborder: 0;\n\t\t\tbox-shadow: 5px 10px 18px #888888;\n\t\t}\n\t}\n\n\n    .page{\n       height: 1000vh;\n       position: relative;\n    }\n    .image{\n        position: fixed;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n    } \n\t.image svg text{\n\t\tposition: absolute;\n\t\ttop: 40%;\n\t\tleft: 50%;\n\t\tz-index: 0;\n\t}\n\n\t#longText{\n\t\tfont-family: var(--text);\n\t\tfont-size: 0.8rem;\n\t}\n\t\n\t.blackImageCover{\n\t\tbackground-color: var(--overLay);\n\t\tposition: fixed;\n\t\topacity: 0;\n\t\theight: 100%;\n\t\twidth: 100%;\n\t}\n\t.toggle .line-1 {\n\t\ttransform: rotate(-45deg) translate(-5px, 5px);\n\t  }\n\t  \n\t  .toggle .line-2 {\n\t\topacity: 0;\n\t  }\n\t  \n\t  .toggle .line-3 {\n\t\ttransform: rotate(45deg) translate(-5px, -5px);\n\t  }\n\n\t  .hamburger-menu {\n\t\tposition: fixed;\n\t\ttop: 80px;\n\t\tright: 80px;\n\t\tmargin: 10px;\n\t\tcursor: pointer;\n\t\tz-index: 2;\n\t  }\n\t  \n\t  .line {\n\t\twidth: 60px;\n\t\theight: 8px;\n\t\tmargin: 10px;\n\t\tbackground-color: #fff;\n\t\t// transition: all 0.3s ease;\n\t  }\n\n\t.downArrow image{\n\t\tfill: white;\n\t}\n\t.downArrow{\n\tposition: fixed;\n\tbottom: 100px;\n\tleft: calc(50% - 50px);\n\tfill: white;\n\tz-index: 2;\n\t}\n\t.bounce {\n\t\t-moz-animation: bounce 2s infinite;\n\t\t-webkit-animation: bounce 2s infinite;\n\t\tanimation: bounce 2s infinite;\n\t}\n\n\timg.logo{\n\t\tz-index: 2;\n\t\tposition: fixed;\n\t\tbottom: 10px;\n\t\tright: 10px;\n\t\tmargin: 10px;\n\t\twidth: 300px;\n\t\t\n\t}\n\n\t.bwa{\n\t\topacity: 0;\n\t}\n\n\n\t@keyframes bounce {\n\t\t0%, 20%, 50%, 80%, 100% {\n\t\t  -moz-transform: translateY(0);\n\t\t  -ms-transform: translateY(0);\n\t\t  -webkit-transform: translateY(0);\n\t\t  transform: translateY(0);\n\t\t}\n\t\t40% {\n\t\t  -moz-transform: translateY(-30px);\n\t\t  -ms-transform: trans